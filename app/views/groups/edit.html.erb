<% content_for :title, t(:rename_group) -%>

<h1><%= content_for :title %></h1>
<% if flash[:notice] -%>
<p class="notice" id="notice">
	<%= flash[:notice] %>
</p>
<% end -%>
<%= form_for @group, :url => { :action => 'update' } do |f| %>
	<%= render :partial => 'form', :locals => { :f => f } %>
<% end %>
